<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
    <Appenders>
        <Console name="Console">
            <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <Root level="info">
            <AppenderRef ref="Console"/>
        </Root>
        <Logger name="pl.robakowski.Launcher.0" level="warn">
            <AppenderRef ref="Console"/>
        </Logger>
        <Logger name="io.activej" level="error">
            <AppenderRef ref="Console"/>
        </Logger>
    </Loggers>
</Configuration>